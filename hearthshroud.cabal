  -- other-modules:       
  other-extensions:    FlexibleInstances, GeneralizedNewtypeDeriving, LambdaCase, TemplateHaskell, TypeSynonymInstances, ViewPatterns, GADTs, KindSignatures, StandaloneDeriving, DeriveDataTypeable, ConstraintKinds, DeriveGeneric, FlexibleContexts, FunctionalDependencies, InstanceSigs, MultiParamTypeClasses, RankNTypes, TypeFamilies, NoMonomorphismRestriction, OverloadedStrings
  build-depends:       base >=4.9 && <4.10, mtl >=2.2 && <2.3, containers >=0.5 && <0.6, lens >=4.15 && <4.16, monad-loops >=0.4 && <0.5, MonadPrompt >=1.0 && <1.1, data-ordlist >=0.4 && <0.5, template-haskell >=2.11 && <2.12, ansi-terminal >=0.6 && <0.7, terminal-size >=0.3 && <0.4, random-shuffle >=0.0 && <0.1, lambda-options >=0.9 && <0.10, process >=1.4 && <1.5, random >=1.1 && <1.2
  hs-source-dirs:      src
  default-language:    Haskell2010
